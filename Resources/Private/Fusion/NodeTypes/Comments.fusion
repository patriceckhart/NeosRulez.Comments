prototype(Site:Comments) < prototype(Neos.Neos:Plugin)
prototype(Site:Comments) {
    package = 'NeosRulez.Comments'
    controller = 'Comment'
    action = 'comments'
    sorting = ${q(node).property('sorting')}
    @cache {
        mode = 'uncached'
        context {
            1 = 'node'
            2 = 'documentNode'
        }
    }
}